{
    "name": "prasadchinwal/laravel-shibboleth",
    "description": "Shibboleth OIDC driver for Laravel Socialite",
    "type": "library",
    "keywords": [
        "Laravel", "Socialite", "Shibboleth", "Laravel Socialite Shibboleth Provider"
    ],
    "support": {
        "issues": "https://github.com/PrasadChinwal/shibboleth/issues",
        "source": "https://github.com/PrasadChinwal/shibboleth"
    },
    "authors": [
        {
            "name": "Chinwal, Prasad"
        }
    ],
    "require": {
        "illuminate/support": "^9.0",
        "illuminate/filesystem": "^9.0",
        "laravel/socialite": "^5.6",
        "laravel/framework": "^9.0"
    },
    "require-dev": {
        "laravel/pint": "^1.10",
        "pestphp/pest": "^1.0",
        "pestphp/pest-plugin-laravel": "^1.0",
        "mockery/mockery": "^1.5",
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^7.24"
    },
    "autoload": {
        "psr-4": {
            "PrasadChinwal\\Shibboleth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PrasadChinwal\\Shibboleth\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PrasadChinwal\\Shibboleth\\ShibbolethServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
